<h2>Users</h2>
<table class="table table-inverse">
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% users.each do |user| %>
      <tr>
        <td><%= image_tag user.get_profile_image(80, 80) %></th>
        <td><%= user.name %></th>
        <td><%= link_to "Show", user_path(user.id) %></th>
      </tr>
    <% end %>
  </tbody>
</table>